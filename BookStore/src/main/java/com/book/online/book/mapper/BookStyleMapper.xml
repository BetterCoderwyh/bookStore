<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.book.online.book.mapper.BookStyleMapper">

     <select id="findById" parameterType="long" resultType="bookstyle">
       select * from bsbookstyle where bookstyleid=#{value}
    </select>
    
    <select id="findAll" resultType="bookstyle">
        select * from bsbookstyle
    </select>
</mapper>
